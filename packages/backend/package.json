{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/src/main",
    "dev": "nest start --watch",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "test": "echo none",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "10.1.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.3",
    "@nestjs/platform-express": "10.1.3",
    "@nestjs/swagger": "7.1.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "formidable": "3.5.1",
    "helmet": "7.0.0",
    "lodash": "4.17.21",
    "nestjs-pino": "3.3.0",
    "pino-http": "8.4.0",
    "pino-pretty": "10.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.12",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@types/express": "4.17.17",
    "@types/formidable": "3.4.2",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.197",
    "@types/node": "20.5.0",
    "@types/supertest": "2.0.12",
    "jest": "29.6.2",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "tsconfig-paths": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src",
      "tests"
    ],
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "setupFiles": [
      "dotenv/config"
    ],
    "testEnvironment": "node"
  }
}
